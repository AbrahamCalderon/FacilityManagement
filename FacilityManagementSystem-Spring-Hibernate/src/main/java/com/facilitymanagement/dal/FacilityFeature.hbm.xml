<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.facilitymanagement.model.facility">
    <class name="FacilityFeature" table="facility_feature">
        <id name="featureId" type="java.lang.Integer" column="feature_id"/>
        <property name="featureDescription" type="java.lang.String" column="feature"/>
    </class>
</hibernate-mapping>